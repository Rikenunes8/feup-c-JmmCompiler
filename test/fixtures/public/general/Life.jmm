    public boolean update() {
        int i;
        int cur;
        int neighN;
        boolean goodPop;
        int[] new_field;

        new_field = new int[_field.length];

        i = 0;
        while (i < _field.length) {
            cur = _field[i];
            neighN = this.getLiveNeighborN(i);
            


            if (!(cur < 1)) {
                goodPop = this.ge(neighN,UNDERPOP_LIM) && this.le(neighN,OVERPOP_LIM);
                if (!goodPop) {
                    new_field[i] = 0;
                } else {
                    new_field[i] = _field[i];
                }
            }

            else {
                if (this.eq(neighN,REPRODUCE_NUM)) {
                    new_field[i] = 1;
                } else {
                    new_field[i] = _field[i];
                }
            }

            i = i + 1;
        }

        _field = new_field;
        return true;

    }

